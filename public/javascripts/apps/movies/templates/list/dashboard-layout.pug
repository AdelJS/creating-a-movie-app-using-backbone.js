.dashboard-filter-bar
.col-xs-9
  #create-movie.row
    .col-xs-12
      button.btn.btn-lg.btn-success Create A New Movie
  #search-box
    form
      .input-group.add-on
        input#search.form-control(type="text" placeholder="movie title" name="filter")
        .input-group-btn
          button.btn.btn-default(type="submit")
            i.glyphicon.glyphicon-search
  table#admin-movies-list.table.table-striped.table-bordered
    thead
      tr
        th Picture
        th Title
        th Date Added
        th Total Watches
        th Favorites
        th Global Activity
        th Action
    tbody.dashboard-view
